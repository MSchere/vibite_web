{
    "name": "vibite",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "prettier --plugin-search-dir . --check . && eslint .",
        "format": "prettier --plugin-search-dir . --write .",
        "build:functions": "npm run --prefix functions build",
        "deploy:functions": "npm run --prefix functions deploy",
        "serve:functions": "npm run --prefix functions serve",
        "emulators:start": "firebase emulators:start --only firestore,auth,functions,hosting"
    },
    "devDependencies": {
        "@iconify/svelte": "^3.1.4",
        "@sveltejs/adapter-auto": "^2.0.0",
        "@sveltejs/adapter-node": "^1.3.1",
        "@sveltejs/adapter-static": "^2.0.3",
        "@sveltejs/kit": "^1.20.4",
        "@typescript-eslint/eslint-plugin": "^5.45.0",
        "@typescript-eslint/parser": "^5.45.0",
        "eslint": "^8.28.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^5.0.0",
        "eslint-plugin-svelte": "^2.30.0",
        "prettier": "^3.0.0",
        "prettier-plugin-svelte": "^3.0.3",
        "sass": "^1.63.6",
        "svelte": "^4.0.0",
        "svelte-adapter-firebase": "^0.15.0",
        "svelte-check": "^3.4.3",
        "svelte-preprocess": "^5.0.4",
        "tslib": "^2.4.1",
        "typescript": "^5.0.0",
        "vite": "^4.4.8"
    },
    "type": "module",
    "dependencies": {
        "@picocss/pico": "^1.5.10",
        "firebase": "^10.0.0"
    }
}
